	description="%Hbird.template.description"
	help="help.html">
	<property-group
		id="chip"
		label="%Hbird.group.target.label"
		description="%Hbird.group.target.description"
		type="PAGES-ONLY"
		help="help.html">
		
		<property
			id="HbirdCore"
			label="%Hbird.core.label"
			description="%Hbird.core.description"
			type="select"
			default="FDV32S301"
			hidden="false"
			persist="true">
			<item          value="s100"         label="FDV32S301" />
			<item          value="FDV32S302"	label="FDV32S302" />
			<item          value="FDV32F003"	label="FDV32F003" />
		</property>
		</property-group>
		
		
		
		<property-group
		id="softwave"
		label="%Hbird.group.target.label"
		description="%Hbird.group.target.description"
		type="PAGES-ONLY"
		help="help.html">
		
		<property
			id="HbirdContent"
			label="%Hbird.content.label"
			description="%Hbird.content.description"
			type="select"
			default="hello"
			hidden="false"
			persist="true">
			<item
				value="empty"
				label="Empty_Project" />
			<item
				value="hello"
				label="Hello_World" />

		</property>

		<!-- ============================================================== -->
		<property
			id="useSomeWarnings"
			label="%someWarnings.label"
			description="%someWarnings.description"
			type="boolean"
			default="true"
			hidden="false"
			persist="true" />
		<property
			id="useMostWarnings"
			label="%mostWarnings.label"
			description="%mostWarnings.description"
			type="boolean"
			default="false"
			hidden="false"
			persist="true" />
		<property
			id="useWerror"
			label="%useWerror.label"
			description="%useWerror.description"
			type="boolean"
			default="false"
			hidden="false"
			persist="true" />
		<property
			id="useOg"
			label="%useOg.label"
			description="%useOg.description"
			type="boolean"
			default="false"
			hidden="false"
			persist="true" />
		<property
			id="useNano"
			label="%useNano.label"
			description="%useNano.description"
			type="boolean"
			default="true"
			hidden="false"
			persist="true" />
	</property-group>
	
	

	<!-- Note: the redundant 'condition="1==1"' is a workaround to control the 
		evaluation order. Without it, processes are evaluated after all ifs. -->
	<if condition="1==1">
		<!-- Create the project, with elf artifact extension. -->
		<process type="org.eclipse.cdt.managedbuilder.core.NewManagedProject">
			<simple
				name="name"
				value="$(projectName)" />
			<simple
				name="artifactExtension"
				value="elf" />
			<simple
				name="isCProject"
				value="true" />
		</process>
	</if>

	<if condition="1==1">
		<!-- Set fileExtension="cpp" if ccnature; default was "c". -->
		<process type="ilg.gnumcueclipse.templates.core.SetPropertyIfHasNature">
			<simple
				name="projectName"
				value="$(projectName)" />
			<simple
				name="natureString"
				value="org.eclipse.cdt.core.ccnature" />
			<simple
				name="propertyName"
				value="fileExtension" />
			<simple
				name="propertyValue"
				value="cpp" />
		</process>

		<!-- Add ccnature always. -->
		<process type="org.eclipse.cdt.core.AddNature">
			<simple
				name="projectName"
				value="$(projectName)" />
			<simple
				name="natureId"
				value="org.eclipse.cdt.core.ccnature" />
		</process>
	</if>

	<if condition="1==1">
		<process type="ilg.gnumcueclipse.templates.core.ConditionalSetProperty">
			<simple
				name="projectName"
				value="$(projectName)" />
			<simple
				name="condition"
				value="" />
			<simple
				name="propertyName"
				value="content" />
			<simple
				name="propertyValue"
				value="$(HbirdContent)" />
		</process>
		<process type="ilg.gnumcueclipse.templates.core.ConditionalSetProperty">
			<simple
				name="projectName"
				value="$(projectName)" />
			<simple
				name="condition"
				value="" />
			<simple
				name="propertyName"
				value="coreName" />
			<simple
				name="propertyValue"
				value="$(HbirdCore)" />
		</process>
	</if>

